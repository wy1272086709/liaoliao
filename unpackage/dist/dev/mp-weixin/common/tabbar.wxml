<view class="tabbar-view" style="{{('padding-bottom:'+bottom+'px;'+'position:'+position+';')}}"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['switchTab',[index]]]]]}}" class="tabbar-navigator" bindtap="__e"><view class="tabbar-icon"><block wx:if="{{index!=current}}"><image class="{{[item.defaultClass]}}" src="{{item.iconPath}}"></image></block><block wx:if="{{index==current}}"><image class="{{[item.defaultClass]}}" src="{{item.hoverPath}}"></image></block></view><view class="tabbar-text"><text>{{item.text}}</text></view></view></block></view>