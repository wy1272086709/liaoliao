
.tabbar-view {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 140rpx;
	background:-webkit-gradient(linear,left top, right top,from(rgba(34,121,239,1)),to(rgba(61,177,242,1)));
	background:-webkit-linear-gradient(left,rgba(34,121,239,1) 0%,rgba(61,177,242,1) 100%);
	background:linear-gradient(90deg,rgba(34,121,239,1) 0%,rgba(61,177,242,1) 100%);
	position: fixed;
	bottom: 0rpx;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
}
.tabbar-icon-class {
	max-width: 48rpx;
	max-height: 48rpx;
}
.tabbar-navigator {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	width:187.5rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tabbar-icon {
}
.tabbar-text {
	height: 40rpx;
	font-size:12rpx;
	font-family:PingFangSC-Regular,PingFang SC;
	font-weight:400;
	color:rgba(255,255,255,1);
	line-height:40rpx;
}

