<view><video class="video" id="{{videoId}}" src="{{url}}" autoplay="true" duration controls="true" danmu-btn="true" enable-danmu="true" loop="{{true}}" muted="{{true}}" initial-time direction="-90" show-mute-btn="true" data-event-opts="{{[['pause',[['pause',['$event']]]],['ended',[['onfinish',['$event']]]],['error',[['onfail',['$event']]]],['waiting',[['waiting',['$event']]]],['timeupdate',[['timeupdate',['$event']]]],['fullscreenchange',[['fullscreenchange',['$event']]]]]}}" bindpause="__e" bindended="__e" binderror="__e" bindwaiting="__e" bindtimeupdate="__e" bindfullscreenchange="__e"></video><button data-event-opts="{{[['tap',[['seek',['$event']]]]]}}" class="btn" bindtap="__e">跳转到指定位置</button></view>