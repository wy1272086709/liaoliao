#tab-scroll {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 50rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  overflow-x: auto;
  width: 686rpx;
  -webkit-overflow-scrolling: touch;
}
#tab-scroll .header-view-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
#tab-scroll .header-view-box view {
  height: 46rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  white-space: nowrap;
  -webkit-flex-basis: 170rpx;
          flex-basis: 170rpx;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  /*margin-right:20px;*/
}
#tab-scroll .header-view-box view view {
  height: 46rpx;
  /*padding-bottom: 5rpx;*/
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  text-align: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
#testing-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-left: 32rpx;
  margin-right: 32rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-top: 20px;
}
#testing-content .testing-box .testing-title-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 330rpx;
  height: 262rpx;
  border: 1px solid black;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
#testing-content .testing-box .testing-title-view .has-pass {
  width: 100%;
  height: 42rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
#testing-content .testing-box .testing-title-view .has-pass view {
  width: 115rpx;
  font-size: 26rpx;
  background-color: red;
  color: #FFFFFF;
  border-bottom-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
#testing-content .testing-box .testing-title-view .testing-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 32rpx;
  color: #FFFFFF;
  background-color: #2D5E70;
  height: 20px;
}
#testing-content .testing-box .testing-stat-view {
  margin-left: 32rpx;
  margin-right: 32rpx;
  margin-top: 20px;
  margin-bottom: 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
#testing-content .testing-box .testing-stat-view .testing-stat-button {
  color: #F18DEC;
  font-size: 24rpx;
  border-radius: 20rpx;
  border: 1px solid #F18DEC;
}
#testing-content .testing-box .testing-stat-view .testing-num {
  font-size: 24rpx;
  color: #F18DEC;
}

