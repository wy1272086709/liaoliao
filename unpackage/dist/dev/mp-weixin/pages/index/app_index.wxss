.tower-swiper {
  height: 420rpx;
  position: relative;
  max-width: 750rpx;
  overflow: hidden;
}
.tower-swiper .tower-item {
  position: absolute;
  width: 300rpx;
  height: 380rpx;
  top: 0;
  bottom: 0;
  left: 50%;
  margin: auto;
  -webkit-transition: all 0.2s ease-in 0s;
  transition: all 0.2s ease-in 0s;
  opacity: 1;
}
.tower-swiper .tower-item.none {
  opacity: 0;
}
.tower-swiper .tower-item .swiper-item {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 6rpx;
          border-radius: 6rpx;
  overflow: hidden;
}
