<view style="{{('min-height:'+height+'px')}}" id="view-content"><view id="title"><view class="title"><text style="font-weight:bold;">{{title}}</text></view><view id="article-info"><view class="author"><text>{{author}}</text></view><view class="readtime"><text>{{readTime}}</text></view></view></view><view class="u-content" id="view-article"><u-parse vue-id="1e5ff8b0-1" html="{{htmls}}" bind:__l="__l"></u-parse></view><view id="bottom"><view id="article-bottom" data-event-opts="{{[['tap',[['thumbup',['$event']]]]]}}" bindtap="__e"><u-icon style="margin-right:17rpx;" vue-id="1e5ff8b0-2" name="thumb-up" color="{{sfsc?'#FF3300':'#A6A6A6'}}" size="40" bind:__l="__l"></u-icon><text>{{wzsccs}}</text></view></view><view id="article-previous-next"><view id="previous-view" data-event-opts="{{[['tap',[['gotoPrev',['$event']]]]]}}" bindtap="__e"><view class="article-link">{{"上一篇: "+(prevTitle?prevTitle:'无')}}</view></view><view id="next-view" data-event-opts="{{[['tap',[['gotoNext',['$event']]]]]}}" bindtap="__e"><view class="article-link">{{'下一篇: '+(nextTitle?nextTitle:'无')+''}}</view></view></view></view>