<view id="root-view"><view id="header-view"><text>登录成功,请完善手机号</text></view><view id="login-area-view"><view id="tel-view"><view class="tel-prefix"><text>+86</text></view><input class="vue-ref" id="phone-id" type="text" name="phone" placeholder="请输入手机号" placeholder-class="text-placeholder-class" data-ref="phoneRef" data-event-opts="{{[['blur',[['validatePhone'],['$forceUpdate']]],['input',[['__set_model',['','phone','$event',['trim']]]]]]}}" value="{{phone}}" bindblur="__e" bindinput="__e"/><view><button class="send-code-btn" disabled="{{disabled}}" data-event-opts="{{[['tap',[['sendCode']]]]}}" bindtap="__e">{{sendMsg}}</button></view></view><view id="error-tel-view"><text>{{telErrorMsg}}</text></view><view id="code-view"><view class="tel-prefix"><text class="validate-code-label">短信验证码</text></view><input class="vue-ref" type="text" id="validate_code" name="code" placeholder="请输入短信验证码" placeholder-class="text-placeholder-class" data-ref="codeRef" data-event-opts="{{[['blur',[['validateCodeStr'],['$forceUpdate']]],['input',[['__set_model',['','validate_code','$event',['trim']]]]]]}}" value="{{validate_code}}" bindblur="__e" bindinput="__e"/></view><view id="error-code-view"><text>{{codeErrorMsg}}</text></view><view id="submit-view" data-event-opts="{{[['tap',[['savePhone',['$event']]]]]}}" bindtap="__e"><button style="border:0px solid #000000;">绑定</button></view></view></view>