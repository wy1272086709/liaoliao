page {
  background-color: #F2F2F2;
}
#root-view {
  margin-top: 40px;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-left: 41rpx;
  padding-right: 41rpx;
  padding-top: 52rpx;
  padding-bottom: 41rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  background-color: #FFFFFF;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-align-content: space-between;
          align-content: space-between;
}
#root-view .score-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 284rpx;
  height: 358rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 284rpx 358rpx;
  position: relative;
}
#root-view .score-item .price-title-view {
  text-align: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #C3AEFF;
}
#root-view .score-item .price-title-view .price-text {
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: 800;
  font-style: italic;
}
#root-view .score-item .price-title-view .price-text:after {
  display: inline-block;
  content: ' ';
  width: 5px;
}
#root-view .score-item .price-desc-view {
  margin-left: 30rpx;
  margin-right: 29rpx;
  color: #CCC;
}
#root-view .score-item .pay-score-view {
  position: absolute;
  bottom: -30rpx;
  left: 38rpx;
  width: 210rpx;
  height: 60rpx;
  background: #C3AEFF;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
#root-view .score-item:nth-child(1), #root-view .score-item:nth-child(2) {
  margin-bottom: 45rpx;
}

