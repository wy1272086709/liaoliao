<view class="tabbar-view" style="{{('padding-bottom:'+bottom+'rpx;'+'position:'+position+';')}}"><block qq:for="{{tabList}}" qq:for-item="item" qq:for-index="index" qq:key="index"><view data-event-opts="{{[['tap',[['switchTab',[index]]]]]}}" class="tabbar-navigator" bindtap="__e"><view class="tabbar-icon"><block qq:if="{{index!=current}}"><image class="{{[item.defaultClass]}}" src="{{item.iconPath}}"></image></block><block qq:if="{{index==current}}"><image class="{{[item.defaultClass]}}" src="{{item.hoverPath}}"></image></block></view><view class="tabbar-text"><text>{{item.text}}</text></view></view></block></view>